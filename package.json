{
  "name": "nyhype.org",
  "private": true,
  "description": "Buy Supreme with crypto",
  "version": "0.0.1",
  "author": "Dzmitry Bachko <dbachko@gmail.com>",
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "cryptocurrency-icons": "^0.18.1",
    "formik": "^2.4.6",
    "gatsby": "^5.12.12",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-amp": "^0.2.2",
    "gatsby-plugin-create-client-paths": "^5.12.0",
    "gatsby-plugin-manifest": "^5.12.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-offline": "^6.12.0",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sass": "^6.12.0",
    "gatsby-plugin-sharp": "^5.12.0",
    "gatsby-source-airtable": "^2.4.4",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-sharp": "^5.12.0",
    "isomorphic-fetch": "^3.0.0",
    "sass": "^1.72.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "spectre.css": "^0.5.9",
    "yup": "^1.3.3"
  },
  "keywords": [
    "ny",
    "hype",
    "supreme"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambda",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:visual": "start-server-and-test develop http://localhost:8000 cy:open",
    "cy:open": "cypress open"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.2.1",
    "babel-jest": "^29.7.0",
    "cypress": "^13.9.0",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "netlify-lambda": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbachko/nyhype.org"
  },
  "bugs": {
    "url": "https://github.com/dbachko/nyhype.org/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "\\.cache",
      "public"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
